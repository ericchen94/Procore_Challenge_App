<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ricky.procore_challenge_app" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity2Activity"
            android:label="@string/title_activity_main_activity2" >
        </activity>
        <activity
            android:name=".PromptNumberInputActivity"
            android:label="@string/title_activity_prompt_number_input"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.ricky.procore_challenge_app.MainActivity" />
        </activity>
        <activity
            android:name=".SieveOfEratosthenesActivity"
            android:label="@string/title_activity_sieve_of_eratosthenes"
            android:parentActivityName=".PromptNumberInputActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.ricky.procore_challenge_app.PromptNumberInputActivity" />
        </activity>
    </application>

</manifest>
